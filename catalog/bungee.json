{
    "name": "BungeeCord Proxy",
    "description": "Minecraft proxy (BungeeCord) for multi-server networks.",
    "docker_image": "cpanel/minecraft-bungeecord:latest",
    "docker_images": {
        "Java 17 (Recommended)": "cpanel/minecraft-bungeecord:latest"
    },
    "startup": "java -Xms128M -Xmx{SERVER_MEMORY}M -jar BungeeCord.jar",
    "environment": {
        "SERVER_MEMORY": "512"
    },
    "ports": [
        {
            "container": 25577,
            "protocol": "tcp"
        }
    ]
}
